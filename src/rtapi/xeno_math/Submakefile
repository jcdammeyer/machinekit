ifeq ($(BUILD_SYS),kbuild)

obj-m += xeno_math.o

ccflags-y := 	$(KERNEL_MATH_CFLAGS) \
	-D_IEEE_LIBM \
	-D_ISOC99_SOURCE \
	-D_SVID_SOURCE

xeno_math-objs += rtapi/xeno_math/libm.o

$(RTLIBDIR)/xeno_math$(MODULE_EXT): $(addprefix $(OBJDIR)/,$(xeno_math-objs))

else

ccflags-y := 	$(CFLAGS) \
	-D_IEEE_LIBM \
	-D_ISOC99_SOURCE \
	-D_SVID_SOURCE

$(RTLIBDIR)/rtapi$(MODULE_EXT): CFLAGS = $(ccflags-y)

rtapi-objs += $(xeno_math-objs)

